[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Deploy to GitHub Pages"
author = 46606100
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add ."

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git commit -m \"Deploy to GitHub Pages: $(date)\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git push origin main"

[[workflows.workflow]]
name = "Run"
author = 46606100
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
